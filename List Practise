friends = ['Tom' , 'Dick' ,  'Harry']
print (friends[0])
print (friends[1])
print (friends[2])

#printing individual messages
message1 = f" Hello {friends[0].title()}"
message2 = f" Yo! {friends[1].title()}"
message3 = f" Whats up {friends[2].title()}"
print(message1)
print(message2)
print(message3)
